{
  "name": "generator-prepare-me-templates-gulp",
  "version": "0.0.6-pre",
  "description": "Scaffolding for templates with Gulp.",
  "author": {
    "name": "jena.hajek",
    "email": "jena.hajek@gmail.com",
    "url": "https://jenahajek.com/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "lint:css": "stylelint generators/app/templates/frontend-src/scss/**/*.scss !generators/app/templates/frontend-src/scss/utils/_normalize.scss !generators/app/templates/frontend-src/scss/docs/*.scss !generators/app/templates/frontend-src/scss/utils/_debug.scss || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jenahajek/generator-prepare-me-templates-gulp"
  },
  "license": "Apache-2.0"
}
